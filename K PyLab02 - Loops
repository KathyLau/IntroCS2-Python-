#4 Challenge
def challenge():
    result = 0
    for i in range(0, 1000):
        if i%3 == 0 or i%5 == 0:
            result += i
    return result
print challenge()
###answer = (sum of multiplies of 3) + (sum of multiplies of 5) - (sum of multiples of 15)
